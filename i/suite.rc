# Check inter-cycle trigger edge case.
# bar needs to auto-spawn but only after the first cycle point.
[cylc]
   cycle point format = %Y
[scheduling]
    initial cycle point = 2000
    final cycle point = 2020
    [[graph]]
        P1Y = """
           foo
           foo[-P1Y] => bar => baz
              """
[runtime]
   [[root]]
      script = sleep 5
